{
  "name": "Wyltl-Language",
  "displayName": "Wyltl Language",
  "description": "Syntax Highlighting for the Wyltl Programming Language",
  "version": "1.0.1",
  "icon": "images/logo.png",
  "engines": {
    "vscode": "^1.98.0"
  },
  "publisher": "LochanaDinethMallawarachchi",
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dineth-lochana/Wyltl-Language.git"
  },
  "contributes": {
    "languages": [{
      "id": "wyltl",
      "aliases": ["Wyltl", "wyltl"],
      "extensions": [".wyltl"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "wyltl",
      "scopeName": "source.wyltl",
      "path": "./syntaxes/wyltl.tmLanguage.json"
    }]
  }
}
